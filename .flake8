[flake8]
max-line-length = 100
extend-ignore = E203, W503, E501
exclude = 
    .git,
    __pycache__,
    .venv,
    venv,
    build,
    dist,
    *.egg-info,
    .pytest_cache,
    app/grpc_service/*_pb2.py,
    app/grpc_service/*_pb2_grpc.py
per-file-ignores =
    __init__.py:F401

